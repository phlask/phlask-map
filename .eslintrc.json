{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended"
  ],
  "rules": {
    "react/prop-types": 0,
    "no-console": 1,
    "no-unused-vars": 0
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    },
    "sourceType": "module",
    "ecmaVersion": "latest",
    "ecmaFeatures": {
      "jsx": true
    },
    "requireConfigFile": false
  },
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
